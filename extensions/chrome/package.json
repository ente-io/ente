    "@types/react-dom": "^19.0.3",
    "@wxt-dev/module-react": "^1.1.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.18.0",
    "jsdom": "^27.4.0",
    "postcss": "^8.5.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3",
